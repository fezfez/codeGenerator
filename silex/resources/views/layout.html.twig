<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="../../docs-assets/ico/favicon.png">
    <title>Code Generator</title>
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/bootstrap.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/shCoreDefault.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/styles.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/nprogress.css">
    <script src="{{ app.base_path }}/assets/js/Vendor/nprogress.js"></script>
    <style type="text/css">
        body > div.navbar , body > div.container-full {
          visibility:hidden;
          opacity: 0;
          transition: opacity 0.5s linear;
        }
    </style>
    <script type="text/javascript">
        Pace.on('hide', function() {
            $('body > div.navbar , body > div.container-full').css({'visibility' : 'visible', 'opacity' : 1});
        });
        __BASEPATH__ = '{{ app.base_path }}';
    </script>
    <script type="text/javascript"
            data-main="{{ app.base_path }}/assets/js/App/Bootstrap.js"
            src="{{ app.base_path }}/assets/js/Vendor/require.js">
    </script>
</head>
<body ng-controller="GeneratorCtrl">
<!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->
    {% set active = active|default(null) %}
    <div class="navbar navbar-inverse" role="navigation">
      <div class="container-full">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Code Generator</a>
        </div>
        <div class="collapse navbar-collapse">
            <form class="navbar-form navbar-right" role="form">
                <div class="form-group">
                    <select ng-class="{hide : (!generatorsList || !metadataName)}"
                            ng-model="generators"
                            ng-options="obj.label as obj.label for obj in generatorsList"
                            class="form-control"
                            name="generators">
                        <option value="">Select Generator</option>
                    </select>
                    <select ng-model="metadataName"
                            ng-class="{hide : !metadataList}"
                            ng-options="obj.id as obj.label for obj in metadataList"
                            class="form-control">
                         <option value="">Select Metadata</option>
                    </select>
                    <select name="backend"
                            class="form-control"
                            ng-model="backEnd"
                            ng-options="obj.id as obj.label for obj in backendList">
                        <option value="">Select Backend</option>
                    </select>
                </div>
                <button class="btn btn-success dropdown-toggle" ng-if="generators != undefined" ng-click="generate()">Generate files !</button>
            </form>
        </div><!--/.nav-collapse -->
      </div>
    </div>
    <div class="container-full row">
        {% block content %}
        {% endblock %}
    </div>
    <!-- Modal -->
    <modal></modal>
    <unsafe-modal></unsafe-modal>
    <waitModal></waitModal>
    <conflict></conflict>
    <metadatasource></metadatasource>
</body>
</html>