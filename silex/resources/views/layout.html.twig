<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="{{ app.base_path }}favicon.ico">
    <title>Code Generator</title>
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/bootstrap.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/shCoreDefault.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/styles.css">
    <link rel="stylesheet" href="{{ app.base_path }}/assets/css/nprogress.css">
    <script src="{{ app.base_path }}/assets/js/Vendor/pace/pace.min.js"></script>
    <style type="text/css">
        body > div.navbar , body > div.container-full {
          visibility:hidden;
          opacity: 0;
          transition: opacity 0.5s linear;
        }
    </style>
    <script type="text/javascript">
        Pace.on('hide', function() {
            $('body > div.navbar , body > div.container-full').css({'visibility' : 'visible', 'opacity' : 1});
        });
        __BASEPATH__ = '{{ app.base_path }}';
    </script>
    <script type="text/javascript"
            data-main="{{ app.base_path }}/assets/js/App/Bootstrap.js"
            src="{{ app.base_path }}/assets/js/Vendor/requirejs/require.js">
    </script>
</head>
<body ng-controller="GeneratorCtrl">
<!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->
    {% set active = active|default(null) %}
    <div class="navbar navbar-inverse" role="navigation">
      <div class="container-full">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Code Generator</a>
        </div>
        <div class="collapse navbar-collapse">
            <form class="navbar-form navbar-right" role="form">
                <div class="form-group">
                <button class="btn btn-success dropdown-toggle" ng-show="fileList" ng-click="generate()">Generate files !</button>
            </form>
        </div><!--/.nav-collapse -->
      </div>
    </div>
    <div class="container">
            {% block content %}
            {% endblock %}
    </div>
    <!-- Modal -->
    <modal></modal>
    <unsafe-modal></unsafe-modal>
    <waitModal></waitModal>
    <newSource>
    {% raw %}
    <div class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">New source</h4>
            </div>
            <div class="modal-body">
                  <div ng-if="configFormError != undefined" class="alert alert-danger fade in">
                       {{ configFormError }}
                  </div>
                    <form ng-submit="backendConfig()">
                        <div class="form-group" ng-repeat="question in configQuestionsList">
                            <label>{{ question.text }}</label>
                            <select ng-if="question.type == 'select'"
                                    id="{{ question.dtoAttribute }}"
                                    class="form-control question"
                                    ng-model="configQuestion[question.dtoAttribute]"
                                    name="{{ question.dtoAttribute }}">
                                    <option ng-repeat="value in question.values"
                                            ng-click="setConfigQuestion(question.dtoAttribute)"
                                            ng-selected="(value.id === question.defaultResponse) ? 'selected' : null"
                                            value="{{ value.id }}" >
                                        {{ value.label }}
                                    </option>
                            </select>
                            <input ng-if="question.type == 'text'"
                                   class="form-control questions"
                                   type="text"
                                   name="{{ question.dtoAttribute }}"
                                   placeholder="{{ question.text }}" ng-model="configQuestion[question.dtoAttribute]" />
                        </div>
                        <input type="submit" class="btn btn-success" value="Configure" />
                    </form>
            </div>
        </div>
    </div>
    </div>
    </newSource>
    <history>
     <div class="modal fade" tabindex="-1" role="dialog">
        <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">History</h4>
            </div>
            <div class="modal-body">
                    <form ng-submit="history()" ng-if="historyCollection != null">
                        <div class="form-group">
                            <label>{{ historyCollection.text }}</label>
                            <select id="{{ historyCollection.dtoAttribute }}"
                                    class="form-control question"
                                    ng-model="historyQuestion[historyCollection.dtoAttribute]">
                                    <option ng-repeat="value in historyCollection.values"
                                            ng-selected="(value.id === historyCollection.defaultResponse) ? 'selected' : null"
                                            value="{{ value.id }}" >
                                        {{ value.label }}
                                    </option>
                            </select>
                        </div>
                        <input type="submit" class="btn btn-success" value="Awake" />
                    </form>
                    <p ng-if="historyCollection == null">Empty history</p>
            </div>
        </div>
    </div>
    </div>
</history>
    <searchgenerator>
     <div class="modal fade" tabindex="-1" role="dialog">
        <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Search generator</h4>
            </div>
            <div class="modal-body">
                <form ng-submit="searchGenerator()">
                    <input type="text" ng-model="searchGeneratorName" />
                    <input type="submit" class="btn btn-info" id="search-generator-btn" value="Search"/>
                </form>
                    <div class="form-group row">
                    	<div class="col-lg-5">
	                        <a href="#"
	                           class="list-group-item col-lg-12"
	                           ng-repeat="generator in searchGeneratorCollection.values"
	                           ng-click="previewGenerator(generator)"
	                           ng-class="{'active': generator.name === backendSelected}">
	                            {{  generator.name }} {{  generator.description }}
	                        </a>
	                        <p ng-if="searchGeneratorCollection == null">No result</p>
                        </div>
                        <div class="col-lg-7" ng-if="generatorPreview !== null">
							{{ generatorPreview.name }}
							<button class="btn btn-success" ng-click="downloadGenerator(generatorPreview.name)">
								Download
							</button>
							<p ng-if="generatorPreviewReadme">{{ generatorPreviewReadme }}</p>
							<div class="console row">
								<i class="fa fa-refresh fa-spin" style="float:right;color:green;" ng-if="downloadEnd == false"></i>
								<i class="glyphicon glyphicon-ok" style="float:right;color:green;" ng-if="downloadEnd == true"></i>
								<p ng-repeat="log in downloadLog" ng-bind-html="log.data" class="col-lg-12">

								</p>
							</div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    </div>
</searchgenerator>
{% endraw %}
    <conflict></conflict>
    <metadatasource></metadatasource>
</body>
</html>