    public function indexAction()
    {
        $serviceManager = $this->getServiceLocator();
        $headTitle = $serviceManager->get('viewHelperManager')->get('headTitle');
        $headTitle->append("Liste des <?php echo $this->entityName; ?>s");

        $<?php echo $this->entityName; ?>DAO = <?php echo $this->ucfirstEntityName; ?>DAOFactory::getInstance($serviceManager);

        return new ViewModel(array(
            'list<?php echo $this->ucfirstEntityName; ?>s' => $<?php echo $this->entityName; ?>DAO->retrieveAll(),
            'flashMessages' => $this->flashMessenger()->getMessages(),
        ));
    }