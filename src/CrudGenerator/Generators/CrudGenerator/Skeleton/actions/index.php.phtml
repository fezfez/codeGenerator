    public function indexAction()
    {
        $serviceManager = $this->getServiceLocator();

        $serviceManager->get('viewHelperManager')->get('headTitle')->append("<?php echo ucfirst($this->dataObject->getDisplayNames()); ?> list");

        $<?php echo $this->entityName; ?>DAO = <?php echo $this->daoFactoryName; ?>::getInstance($serviceManager);

        return new ViewModel(array(
            'list<?php echo $this->ucfirstEntityName; ?>s' => $<?php echo $this->entityName; ?>DAO->findAll(),
            'flashMessages' => $this->flashMessenger()->getCurrentSuccessMessages(),
        ));
    }
